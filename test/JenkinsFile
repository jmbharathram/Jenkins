pipeline {
    agent {
        docker {
            image 'alpine:3.14'
            args '--user root -v /var/run/docker.sock:/var/run/docker.sock'
        }
    }
    stages {
        stage('Clone') {
            steps {
                sh 'git clone git@github.com:jmbharathram/SpringAPI.git'
            }
        }
    }
}
